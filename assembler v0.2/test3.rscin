start:
sub r0, r0, r0  // r0 = 0000000000000000
ld  r3, r0(0)   // r3 = mem 0 = 1000_0000_0000_0001
ld  r4, r0(1)   // r4 = mem 1 = 1000_0000_0000_0010
ld  r5, r0(2)   // r5 = mem 2 = 1000_0000_0000_0100
ld  r1, r3(0)   // r1 = io 1
ld  r2, r4(0)   // r2 = io 2
or  r6, r1, r2  // r6 = r1 | r2
st  r6, r5(0)   // io 4 = r6
jmp 0
