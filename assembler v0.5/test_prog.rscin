// This can be single stepped in the testbench

start:
0	ld  x3, x0(0)
4	ld  x1, x0(4)
8	add x2, x3, x1
12	st  x2, x0(4)
16	sub x2, x3, x1
20	inv x2, x3
24	lsl x2, x3, x1
28	lsr x2, x3, x1
32	and x2, x3, x1
36	or  x2, x3, x1
40	slt x2, x3, x1
44	add x3, x3, x3
48	beq x3, x2, 12
52	bne x3, x2, 8
56	jmp start
60	jmp -44