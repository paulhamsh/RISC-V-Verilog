start:
sub x0, x0, x0  // x0 = 0000000000000000
ld  x3, x0(0)   // x3 = mem 0 = 1000_0000_0000_0001
ld  x4, x0(1)   // x4 = mem 1 = 1000_0000_0000_0010
ld  x5, x0(2)   // x5 = mem 2 = 1000_0000_0000_0100
ld  x1, x3(0)   // x1 = io 1
ld  x2, x4(0)   // x2 = io 2
or  x6, x1, x2  // x6 = x1 | x2
st  x6, x5(0)   // io 4 = x6
jmp start
